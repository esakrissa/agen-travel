# Traefik Sensitive Configuration
# DO NOT COMMIT THIS FILE TO VERSION CONTROL
# Copy this file to .env and fill in your actual values

# Dashboard Credentials
TRAEFIK_DASHBOARD_USER=admin
TRAEFIK_DASHBOARD_PASSWORD=your-traefik-password

# Monitoring Credentials
# Grafana Dashboard
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your-grafana-password

# Prometheus Dashboard
PROMETHEUS_ADMIN_USER=admin
PROMETHEUS_ADMIN_PASSWORD=your-prometheus-password

# API Keys (untuk future use)
# TRAEFIK_API_KEY=your-api-key-here

# SSL/TLS Configuration (untuk production)
# ACME_EMAIL=admin@your-domain.com
# CLOUDFLARE_API_TOKEN=your-cloudflare-token

# Security Configuration
RATE_LIMIT_API=100
RATE_LIMIT_WEBHOOK=10
RATE_LIMIT_DASHBOARD=30
RATE_LIMIT_MONITORING=50
